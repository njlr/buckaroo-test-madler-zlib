[project]
  ignore = .git, .buckd

[repositories]
  google.gtest = buckaroo/official/google/gtest
  madler.zlib = buckaroo/official/madler/zlib

[cxx]
  untracked_headers = error
  untracked_headers_whitelist = /usr/include/.*, /usr/local/Cellar/llvm/4.0.1/.*, /usr/lib/gcc/.*
  should_remap_host_platform = true

